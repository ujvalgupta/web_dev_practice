// Pick only given keys from the object

const obj = {
    A : [3,4,5],
    B : [7,8],
    C : [9,10]
}

const pickArray = ["A","C"]
const ansObj = {}

for ( const [key,value] of Object.entries(obj))
{
    if(pickArray.indexOf(key) != -1)
    {
        ansObj[key] = value;
    }
}

console.log(ansObj)